{
  "name": "@l2beat/config",
  "version": "0.1.0",
  "private": true,
  "types": "./build/src/index.d.ts",
  "exports": {
    ".": {
      "default": "./build/src/index.js"
    },
    "./projects": "./build/src/resolved/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf build",
    "format:fix": "biome format --write .",
    "format": "biome format .",
    "lint:fix": "biome check --formatter-enabled=false --write .",
    "lint": "biome check --formatter-enabled=false .",
    "test": "mocha",
    "typecheck": "tsc --noEmit",
    "get-lz-data": "node -r esbuild-register scripts/omnichain",
    "update-multichain": "node -r esbuild-register scripts/multichain",
    "socket-crawl": "node -r esbuild-register scripts/socketcrawl",
    "socket-convert": "node -r esbuild-register scripts/socketconvert",
    "fill-ts": "node -r esbuild-register scripts/fillts",
    "check-verified-contracts": "node -r esbuild-register scripts/checkVerifiedContracts",
    "tokens": "node -r esbuild-register scripts/tokens/tokens.ts",
    "tokens:verify": "sh scripts/tokens/verify.sh",
    "coingecko:platforms": "node -r esbuild-register scripts/getCoingeckoPlatforms.ts",
    "stargatev2-pools": "node -r esbuild-register scripts/stargatev2-pools",
    "json": "node -r esbuild-register src/resolved/index.ts",
    "generate-projects-json": "node -r esbuild-register src/projects/layer2s/generate-l2s-json.ts && node -r esbuild-register src/projects/bridges/generate-bridges-json.ts && node -r esbuild-register src/projects/layer3s/generate-l3s-json.ts && node -r esbuild-register src/projects/other/zk-catalog/generate-zk-catalog-projects-json.ts && node -r esbuild-register src/projects/other/da-beat/generate-da-beat-projects-json.ts && node -r esbuild-register src/chains/generate-chains-json.ts"
  },
  "dependencies": {
    "@l2beat/backend-tools": "workspace:*",
    "@l2beat/discovery": "workspace:*",
    "@l2beat/discovery-types": "workspace:*",
    "@l2beat/shared": "workspace:*",
    "@l2beat/shared-pure": "workspace:*",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.11",
    "dotenv": "^16.4.5",
    "ethers": "^5.7.2",
    "jsonc-parser": "^3.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "superjson": "^2.2.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@l2beat/typescript-config": "workspace:*",
    "@types/lodash": "^4.17.12",
    "@types/node-fetch": "^2.6.11",
    "p-limit": "^6.1.0"
  }
}
